{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "windows-terminal",
  "title": "Windows Terminal",
  "description": "Launch and manage Windows Terminal instances",
  "icon": "extension-icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "open-profile",
      "title": "Open Profile",
      "description": "Creates a new Windows Terminal instance for a profile",
      "mode": "view",
      "subtitle": "Windows Terminal"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish"
  },
  "author": "lunaris",
  "platforms": [
    "Windows"
  ],
  "categories": [
    "Developer Tools",
    "Productivity",
    "System"
  ]
}
