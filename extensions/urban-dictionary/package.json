{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "urban-dictionary",
  "title": "Urban Dictionary Search",
  "description": "Search on Urban Dictionary",
  "icon": "urban.png",
  "author": "pernielsentikaer",
  "platforms": [
    "macOS",
    "Windows"
  ],
  "contributors": [
    "zhenpewu",
    "xilopaint"
  ],
  "categories": [
    "Web",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Urban Dictionary",
      "description": "Lookup terms on Urban Dictionary",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "type": "text",
          "required": false,
          "placeholder": "Query"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.104.1",
    "@raycast/utils": "^2.2.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "2.1.1",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
